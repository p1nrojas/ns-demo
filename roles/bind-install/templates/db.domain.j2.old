$TTL 604800
@ IN SOA ntp.{{ dns.domain }}. admin.{{ dns.domain }} (
                            1 ; Serial
                            604800 ; Refresh
                            86400 ; Retry
                            2419200 ; Expire
                            604800 ) ; Negative Cache TTL
;
; name servers – NS records

{{ dns.domain }}. IN NS ns01.{{ dns.domain }}.

; name servers – A records
ns01.{{ dns.domain }}. IN A {{ nserver.if_mgmt.ip  }}

