vsc1.{{ dns.domain }}. IN A 10.10.10.6
vsc2.{{ dns.domain }}. IN A 10.10.10.7
vrsg.{{ dns.domain }}. IN A 10.10.10.7
master.{{ dns.domain }}. IN A 10.10.10.11
minion01.{{ dns.domain }}. IN A 10.10.10.12
minion02.{{ dns.domain }}. IN A 10.10.10.20
